<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Price</th>
      <th scope="col">Thumbnail</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>{{title}}</td>
      <td>${{price}}</td>
      <td>
        <img class="thumbnail" src={{thumbnail}} />
      </td>
    </tr>
  </tbody>
</table>